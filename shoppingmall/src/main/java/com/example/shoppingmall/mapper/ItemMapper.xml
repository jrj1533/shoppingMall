<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.shoppingmall.mapper.ItemMapper">
	<insert id="insertItem">
		INSERT INTO item (username,option_no,item_title,item_content,item_amount) VALUES (#{item.username},#{num},#{item.itemTitle},#{item.itemContent},#{item.itemAmount})
	</insert>
</mapper>